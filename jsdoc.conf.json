{
    "source": {
      "include": ["src"],
      "includePattern": ".+\\.jsx?$",
      "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
      "plugins/markdown"
    ],
    "templates": {
      "cleverLinks": false,
      "monospaceLinks": false
    },
    "opts": {
      "recurse": true,
      "destination": "./docs"
    }
  }
  